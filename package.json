{
  "name": "the-house-protocol",
  "private": true,
  "workspaces": ["web", "backend"],
  "scripts": {
    "dev": "bun run --filter '*' dev",
    "dev:web": "bun run --cwd web dev",
    "dev:backend": "bun run --cwd backend dev",
    "build": "bun run --filter '*' build",
    "build:web": "bun run --cwd web build",
    "lint": "bun run --filter '*' lint",
    "typecheck": "bun run --filter '*' typecheck",
    "compile": "bun run --cwd contract compile",
    "test:contract": "bun run --cwd contract test",
    "deploy": "bun run --cwd contract deploy"
  },
  "packageManager": "bun@1.1.0",
  "dependencies": {
    "nitro": "npm:nitro-nightly@^3.0.1-20260202-124820-1954b824"
  },
  "overrides": {
    "nitro": "npm:nitro-nightly@^3.0.1-20260202-124820-1954b824"
  }
}
